        <activity
            android:name="csms.edu.mn.devis.ubbuilding.Office"
            android:label="@string/title_activity_office" >
        </activity>
        <activity
            android:name="csms.edu.mn.devis.ubbuilding.News"
            android:label="@string/title_activity_news" >
        </activity>
        <activity
            android:name="csms.edu.mn.devis.ubbuilding.PriceInfo"
            android:label="@string/title_activity_price_info" >
        </activity>
        <activity
            android:name="csms.edu.mn.devis.ubbuilding.Calculate"
            android:label="@string/title_activity_calculate" >
